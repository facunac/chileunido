@CHARSET "ISO-8859-1";
div.MoreOptionsContainer
{
}
div.MoreOptionsContainer div
{
	border: 1px solid white;
	margin: 10px auto;
	width: 80%;
}
div.MoreOptionsContainer div h3
{
	border-bottom: 2px solid black;
	width: 90%;
	margin: 10px;
	padding: 10px;
	text-align: left;
}
div.MoreOptionsContainer div ul
{
	display: block;
	
}
div.MoreOptionsContainer div ul li a
{
	text-decoration:  none;
	color: black;
}
div.MoreOptionsContainer div ul li
{
	display:block;
	border: 1px solid black;
	background-color: #f3f3f3;
	color: black;
	font-size: 24pt;
	font-family: Bodoni Verdana Arial sans-serif;
	width: 300px;
	margin: 10px;
	text-align: center;
	vertical-align: middle;
}
div.MoreOptionsContainer div ul li:hover
{
	background-color: silver;
	color: white !important;
	
}